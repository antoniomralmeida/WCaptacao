<!DOCTYPE html>
<html lang="pt">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <title>@ViewBag.Title W Captação</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body class="container">

    <header class="row navbar navbar-primary">


        <div class="navbar-header ">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>


            <img src="~/Content/imagens/logoApp.jpg" />


        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">


                <li>@Html.ActionLink("Início", "Index", "Home", new { area = "" }, null)</li>
                <li>@Html.ActionLink("Sobre", "Index", "Help", new { area = "HelpPage" }, null)</li>

            </ul>

            @Html.Partial("_LoginPartial")

        </div>

    </header>
    <div class="container body-content">
        @RenderBody()
        @RenderSection("SPAViews", required: false)
        <hr />
        <footer></footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("Scripts", required: false)
</body>
</html>
